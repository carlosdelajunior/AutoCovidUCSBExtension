{
    "manifest_version": 2,
    "name": "Auto UCSB Covid Survey Filler",
    "description": "Auto fill that Covid Survey you lazy monkey",
    "version": "1.0",
    "author": "Carlos dela Junior",
    "browser_action": {
        "default_title": "Auto UCSB Covid Survey Filler"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://studenthealthoc.sa.ucsb.edu/",
        "storage"
    ],
         
    "background": {
        "scripts": [
            "background.js"
        ]
    }, 
    "icons": {
        "64" : "./icon1.png",
        "128" : "./icon1.png"}
    
     
}